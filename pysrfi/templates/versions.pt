<html metal:use-macro="load: main.pt">
    <div metal:fill-slot="content">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
                <h1>Title: </h1>
                ${title}
            </div>
          </div>
          <div tal:repeat="item versions" class="row">
            <div class="col-md-12">
                <a href="@@version/${item[1]}">${item[0]}</a>
            </div>
          </div>
        </div>
    </div>
</html>
